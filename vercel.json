{
  "version": 2,
    "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ]
}
